{
  "name": "react-native-ios-scroll-picker",
  "version": "1.0.0",
  "description": "Scroll picker like ios",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "lint": "tslint --fix -p ./tsconfig.json",
    "prepare": "bob build",
    "tsc": "tsc --noEmit"
  },
  "author": "Dmitry Chistik",
  "keywords": [
    "react-native",
    "react",
    "native",
    "appsflyer"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": ">=2.0.0-rc.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.21",
    "@types/react-native": "^0.65.0",
    "eslint": "^7.32.0",
    "react": "^17.0.2",
    "react-native": "^0.65.1",
    "react-native-builder-bob": "^0.18.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.2",
    "typescript": "^4.4.3"
  },
  "main": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "lib/**/*"
  ],
  "react-native": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dchystsik/react-native-ios-scroll-picker.git"
  },
  "module": "lib/module/index.js",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "license": "MIT"
}
